  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "babel-eslint": "^10.1.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",

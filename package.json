{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ENVIRONMENT=envs/.env.development node server.js",
    "build": "ENVIRONMENT=envs/.env.staging next build",
    "build:prod": "ENVIRONMENT=envs/.env.production next build",
    "start": "ENVIRONMENT=envs/.env.staging next start -p $PORT",
    "start:dev": "ENVIRONMENT=envs/.env.development next start",
    "start:prod": "ENVIRONMENT=envs/.env.production NODE_ENV=production next start -p 5050",
    "optimize-svg": "svgo --config=.svgo.yml -f ./public/icons",
    "convert-svg": "svgr -d ./components/icon ./public/icons && npm run format:icons",
    "lint": "eslint --fix .",
    "format:icons": "prettier --write ./components/icon/*js --config ./.prettierrc",
    "format": "prettier --write . --config ./.prettierrc"
  },
  "engines": {
    "npm": "6.x",
    "node": "^12.0.0"
  },

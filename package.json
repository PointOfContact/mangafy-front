{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ENVIRONMENT=envs/.env.development next dev",
    "build": "ENVIRONMENT=envs/.env.staging next build",
    "build:prod": "ENVIRONMENT=envs/.env.production next build",
    "start": "ENVIRONMENT=envs/.env.staging next start -p $PORT",
    "start:dev": "ENVIRONMENT=envs/.env.development next start",
    "start:prod": "ENVIRONMENT=envs/.env.production NODE_ENV=production next start -p 5050",
    "optimize-svg": "svgo --config=.svgo.yml -f ./public/icons",
    "convert-svg": "svgr -d ./components/icon ./public/icons && npm run format:icons",
    "lint": "eslint --fix .",
    "format:icons": "prettier --write ./components/icon/*js --config ./.prettierrc",
    "format": "prettier --write . --config ./.prettierrc"
  },
  "engines": {
    "npm": "6.x",
    "node": "^12.0.0"
  },
  "plugins": [],
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@feathersjs/authentication-client": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/rest-client": "^4.5.11",
    "@fingerprintjs/fingerprintjs": "^3.3.1",
    "@imgix/js-core": "^3.6.0",
    "@next/bundle-analyzer": "^10.0.8",
    "@paypal/react-paypal-js": "^6.0.1",
    "@sentry/browser": "^5.30.0",
    "@sentry/node": "^5.30.0",
    "@typeform/embed": "^1.35.1",
    "amplitude": "^6.0.0",
    "amplitude-js": "^8.13.1",
    "antd": "^4.20.7",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.27.2",
    "classnames": "^2.2.6",
    "cookie": "^0.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dotenv": "^10.0.0",
    "moment": "^2.29.3",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.24.0",
    "nextjs-sitemap-generator": "^1.1.3",
    "node-fetch": "^2.6.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "queue-promise": "^2.2.1",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.2",
    "react-avatar-edit": "^1.0.0",
